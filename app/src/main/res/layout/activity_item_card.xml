<?xml version="1.0" encoding="utf-8"?>
<androidx.cardview.widget.CardView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="300dp"
    xmlns:app="http://schemas.android.com/apk/res-auto"

    android:id="@+id/cardview"
    app:cardElevation="4dp"
    app:cardCornerRadius="20dp"
    app:cardUseCompatPadding="true"
    app:contentPadding="15dp">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="241dp"
        android:orientation="horizontal">

        <TextView
            android:id="@+id/lbautorDeTicket"
            android:layout_width="143dp"
            android:layout_height="29dp"
            android:text="Text"
            android:textSize="18sp"
            tools:layout_editor_absoluteX="25dp"
            tools:layout_editor_absoluteY="153dp" />

        <TextView
            android:id="@+id/lbestadoDeTicket"
            android:layout_width="143dp"
            android:layout_height="29dp"
            android:text="Text"
            android:textSize="18sp"
            tools:layout_editor_absoluteX="191dp"
            tools:layout_editor_absoluteY="109dp" />

        <TextView
            android:id="@+id/lbdescripcionDeTicket"
            android:layout_width="143dp"
            android:layout_height="29dp"
            android:text="Text"
            android:textSize="18sp"
            tools:layout_editor_absoluteX="25dp"
            tools:layout_editor_absoluteY="109dp" />

        <TextView
            android:id="@+id/lbtituloDeTicket"
            android:layout_width="143dp"
            android:layout_height="29dp"
            android:text="Text"
            android:textSize="18sp"
            tools:layout_editor_absoluteX="25dp"
            tools:layout_editor_absoluteY="66dp" />

        <TextView
            android:id="@+id/lbuuid"
            android:layout_width="143dp"
            android:layout_height="29dp"
            android:text="Text"
            android:textSize="18sp"
            tools:layout_editor_absoluteX="25dp"
            tools:layout_editor_absoluteY="19dp" />

        <TextView
            android:id="@+id/lbemailDeAutor"
            android:layout_width="143dp"
            android:layout_height="29dp"
            android:text="Text"
            android:textSize="18sp"
            tools:layout_editor_absoluteX="191dp"
            tools:layout_editor_absoluteY="19dp" />

        <TextView
            android:id="@+id/lbfechaDeCreacionDeTicket"
            android:layout_width="143dp"
            android:layout_height="29dp"
            android:text="Text"
            android:textSize="18sp"
            tools:layout_editor_absoluteX="191dp"
            tools:layout_editor_absoluteY="66dp" />

        <TextView
            android:id="@+id/lbfechaDeFinalizacionDeTicket"
            android:layout_width="143dp"
            android:layout_height="29dp"
            android:text="Text"
            android:textSize="18sp"
            tools:layout_editor_absoluteX="191dp"
            tools:layout_editor_absoluteY="153dp" />

    </androidx.constraintlayout.widget.ConstraintLayout>
</androidx.cardview.widget.CardView>